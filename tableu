SYNTAX
NAME = CODE
IF STATEMENT
IF(<logical_test>, <value_if_true>[, <value_if_false>])
EX
Price Group =
IF(
    'Product'[List Price] < 500,
    "Low",
    IF(
        'Product'[List Price] < 1500,
        "Medium",
        "High"
    )
)

